{
  "name": "control-center",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:reset": "npm run cleanup:dev && npm run db:migrate && npm run dev",
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --config eslint.config.mjs",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --config eslint.config.mjs",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "npx tsx prisma/seed.ts",
    "db:seed-test": "npx tsx scripts/seed-test-data.ts",
    "db:update-admin-defaults": "npx tsx scripts/update-admin-defaults.ts",
    "cleanup:dev": "npx tsx scripts/cleanup-dev.ts",
    "test": "vitest",
    "test:watch": "npx vitest watch",
    "test:coverage": "npx vitest run --coverage",
    "test:ui": "npx vitest --ui",
    "test:integration": "cross-env TEST_TYPE=integration vitest run **/*.integration.test.ts **/*.db.test.ts",
    "test:integration:watch": "cross-env TEST_TYPE=integration vitest watch **/*.integration.test.ts **/*.db.test.ts",
    "test:integration:ui": "cross-env TEST_TYPE=integration vitest --ui **/*.integration.test.ts **/*.db.test.ts",
    "test:db:clear": "rimraf .test-db",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "test:e2e:debug": "npx playwright test --debug",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "prisma": {
    "seed": "npx tsx prisma/seed.ts"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.10",
    "@mui/material": "^6.4.10",
    "@prisma/client": "^6.5.0",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "archiver": "^7.0.1",
    "bcryptjs": "^3.0.2",
    "date-fns": "^4.1.0",
    "extract-zip": "^2.0.1",
    "formidable": "^3.5.2",
    "jsonwebtoken": "^9.0.2",
    "next": "15.2.3",
    "next-auth": "^4.24.11",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/eslintrc": "^3",
    "@next/eslint-plugin-next": "^15.2.4",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/archiver": "^6.0.3",
    "@types/bcryptjs": "^2.4.6",
    "@types/formidable": "^3.4.5",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.28",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.1.3",
    "conventional-changelog-conventionalcommits": "^8.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "msw": "^2.7.3",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^3.1.3",
    "vitest-mock-extended": "^3.1.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
